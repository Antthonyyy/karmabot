{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cd client && vite",
    "build": "cd client && vite build && cd .. && rm -rf dist/public && mkdir -p dist/public && cp -r client/dist/* dist/public/ && esbuild server/index.ts --bundle --platform=node --format=esm --packages=external --outdir=dist",
    "build:analyze": "cd client && vite build --mode=analyze",
    "start": "node dist/index.js",
    "check": "tsc",

    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "bundle:analyze": "npx vite-bundle-analyzer",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx server/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "deploy:staging": "npm run build && npm run db:migrate",
    "deploy:production": "npm run audit:security && npm run build && npm run db:migrate",
    "health:check": "curl -f http://localhost:5000/api/health || exit 1"
  },
